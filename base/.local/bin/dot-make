#! /bin/bash

for template in $(find $HOME/.dotfiles -name *.dot_template); do
    pushd $(dirname $template) >/dev/null
    file=$(basename $template .dot_template)
    echo "Creating \"$PWD/$file\""
    $HOME/.local/bin/mo --source=$HOME/.config/base/mo.env $template > $file
    popd >/dev/null
done
