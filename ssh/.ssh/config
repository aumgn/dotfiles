AddKeysToAgent yes

Host github github.com
   HostName github.com
   User git
   IdentityFile ~/.ssh/id_rsa.github

Host mandelbrot mandelbrot.e.ujf-grenoble.fr
   HostName mandelbrot.e.ujf-grenoble.fr
   ForwardX11 yes
   User ferreich
   IdentityFile ~/.ssh/id_rsa.ujf

Host frontend erods-frontend.ujf-grenoble.fr
   Hostname erods-frontend.ujf-grenoble.fr
   User ferreira
   IdentityFile ~/.ssh/id_rsa.erods-frontend

Host mc2 erods-mc-2.ujf-grenoble.fr
   Hostname erods-mc-2.ujf-grenoble.fr
   User ferreira
   IdentityFile ~/.ssh/id_rsa.erods-frontend
   ProxyCommand ssh -W %h:%p frontend
